<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Mapper.userLikedMapper">

    <select id="selectAll" resultType="POJO.news">
        select * from News where ID = #{ID};
    </select>

    <select id="selectByChannelID" resultType="POJO.news">
        select ID,channelID,title,cover,description,postedTime from news where channelID=#{ID};
    </select>

</mapper>